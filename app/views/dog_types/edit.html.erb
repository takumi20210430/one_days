<div class="container py-5">
  <h5>犬種編集</h5>
    <div class="row">
      <div class="col-10 py-5">
        <%= form_with model:@dog_type, local:true do |f| %>
          <%= f.label :type_name, '犬種' %>
          <%= f.text_field :type_name,class: "mx-5"%>
          <%= f.submit '変更を保存',class: 'btn btn-success' %>
        <% end %>
      </div>
    </div>
</div>
