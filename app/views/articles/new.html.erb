<h2 class="mt-3">愛犬の追加</h2>
<%= form_with model: @article, local:true do |f| %>
  <div class="form-group">
    <label for="article">title</lebel>
    <br><%= f.text_field :title, class: "col-md-12" %></br>
  </div>
  <div class="form-group">
    <label for="article_body">投稿内容</lebel>
    <br><%= f.text_field :body, class: "col-md-12" %></br>
  </div>
  <div class="form-group">
    <%= f.attachment_field :image1 %>
  </div>
  <div class="form-group">
    <%= f.attachment_field :image2 %>
  </div>
  <div class="form-group">
    <%= f.attachment_field :image3 %>
  </div>
  <div class="form-group">
    <%= f.attachment_field :image4 %>
  </div>
  <div class="form-group">
    <%= f.attachment_field :image5 %>
  </div>
  <div class="form-group">
    <br><%= f.submit '投稿' %></br>
  </div>
<% end %>